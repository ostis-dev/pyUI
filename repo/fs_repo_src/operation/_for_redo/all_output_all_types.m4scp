//#label(aoc_def_sameSheet)

callReturn([ 
	1_: fixed_: ui_Output,
	2_: fixed_: {[
		1_: answer_reciever,
		2_: result
	]}
])

//#eraseEl(1_:fixed_: result)
return()
end
