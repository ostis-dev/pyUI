])

searchSetStr3([
	set2_: set3_: fixed_: result,
		1_: fixed_: elForProcess,
		2_: assign_: arc_: pos_: const_: t1,
		3_: assign_: t2
])

genElStr3([
	1_:fixed_: result,
	2_:assign_:arc_:pos_:const_: t1,
	3_:fixed_: elForProcess
])

//#ifVarAssign([1_: shift],aoc_def_sameSheet,)
